{
  "name": "interbox-v2",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:mvp": "tsc --noEmit --skipLibCheck && vite build",
    "build:fast": "vite build --mode development",
    "build:check": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "generate-types": "npx supabase gen types typescript --project-id $PROJECT_ID > src/types/supabase.ts",
    "generate-types:local": "npx supabase gen types typescript --local > src/types/supabase.ts"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.45.0",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.39.4",
    "@types/leaflet": "^1.9.20",
    "browser-image-compression": "^2.0.2",
    "canvas-confetti": "^1.9.3",
    "chart.js": "^4.5.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^11.18.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.540.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.0",
    "vite-plugin-pwa": "^1.0.3",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.4.3",
    "vite": "^7.1.3"
